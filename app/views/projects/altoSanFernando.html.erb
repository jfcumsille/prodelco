<%provide(:title, "Alto San Fernando")%>
</br></br></br>

<div class = "row">	
	<div class="col-md-6">
		<div id="arauCarous" class="carousel slide" data-ride="carousel">		
			<!-- Indicators -->
			<ol class="carousel-indicators">
				<li data-target="#arauCarous" data-slide-to="0" class="active"></li>
				<li data-target="#arauCarous" data-slide-to="1"></li>
				<li data-target="#arauCarous" data-slide-to="2"></li>
			</ol>

			<!-- Wrapper for slides -->
			<div class="carousel-inner" role="listbox" >
				<div class="item active">
		      		<%= image_tag "alto1.JPG", class:"image-carousel"%>
		    	</div>

			    <div class="item">
			    	<%= image_tag "alto2.JPG", class:"image-carousel"%>
			    </div>

		    	<div class="item">
		      		<%= image_tag "alto3.JPG", class:"image-carousel"%>
		    	</div>
			</div>

			<!-- Left and right controls -->
			<a class="left carousel-control" href="#arauCarous" role="button" data-slide="prev">
				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
				<span class="sr-only">Previous</span>
			</a>
			<a class="right carousel-control" href="#arauCarous" role="button" data-slide="next">
				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
				<span class="sr-only">Next</span>
			</a>
		</div>
	</div>
	<!-- Video -->
	<div class = "col-md-6">
		<div id="vimeo">
			<iframe src = "https://player.vimeo.com/video/92934907" height= "320" width="560" frameborder= "0" webkitllowfullscreen moazllowfullscreen allowfullscreen></iframe>
		</div>
		<h2 style="color:black; font-weight:bold; font-family:Arial;">Alto San Fernando</h2> 
	</div>
	
</div>

